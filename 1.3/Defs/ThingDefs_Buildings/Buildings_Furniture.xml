<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <ThingDef ParentName="TableGatherSpotBase">
    <defName>VFEE_GrandTable</defName>
    <label>grand table</label>
    <graphicData>
      <texPath>Furniture/TableGrand3x6</texPath>
      <graphicClass>Graphic_Multi</graphicClass>
      <drawSize>(5, 8)</drawSize>
      <damageData>
        <cornerTL>Damage/Corner</cornerTL>
        <cornerTR>Damage/Corner</cornerTR>
        <cornerBL>Damage/Corner</cornerBL>
        <cornerBR>Damage/Corner</cornerBR>
      </damageData>
	  <shaderType>CutoutComplex</shaderType>
    </graphicData>
    <uiIconScale>0.9</uiIconScale>
    <statBases>
      <MaxHitPoints>200</MaxHitPoints>
      <WorkToBuild>50000</WorkToBuild>
      <Mass>25</Mass>
      <Flammability>1.0</Flammability>
      <Beauty>100</Beauty>
    </statBases>
    <size>(3,6)</size>
    <costStuffCount>200</costStuffCount>
	<costList>
	    <Gold>10</Gold>
	</costList>
    <building>
      <buildingSizeCategory>Medium</buildingSizeCategory>
    </building>
    <researchPrerequisites>
      <li>VFEE_NobleDecorations</li>
    </researchPrerequisites>
  </ThingDef>
 
  <ThingDef ParentName="ArtableFurnitureBase">
    <defName>VFEE_GrandChair</defName>
    <label>grand chair</label>
    <description>text.todo</description>
    <graphicData>
      <texPath>Things/Building/Furniture/DiningChair</texPath>
      <graphicClass>Graphic_Multi</graphicClass>
      <drawSize>(3,3)</drawSize>
      <damageData>
        <rectN>(0.25,0.234375,0.5,0.28125)</rectN>
        <rectS>(0.25,0.234375,0.5,0.28125)</rectS>
        <rectE>(0.28125,0.25,0.453125,0.25)</rectE>
        <rectW>(0.28125,0.25,0.453125,0.25)</rectW>
      </damageData>
      <shadowData>
        <volume>(0.23, 0.26, 0.23)</volume>
      </shadowData>
    </graphicData>
    <altitudeLayer>Building</altitudeLayer>
    <statBases>
      <MaxHitPoints>100</MaxHitPoints>
      <WorkToBuild>10000</WorkToBuild>
      <Mass>8</Mass>
      <Flammability>1.0</Flammability>
      <Beauty>80</Beauty>
      <Comfort>0.70</Comfort>
    </statBases>
    <socialPropernessMatters>true</socialPropernessMatters>
    <stuffCategories>
      <li>Metallic</li>
      <li>Woody</li>
	  <li>Stony</li>
    </stuffCategories>
    <costStuffCount>100</costStuffCount>
    <pathCost>30</pathCost>
    <fillPercent>0.35</fillPercent>
    <defaultPlacingRot>South</defaultPlacingRot>
    <building>
      <isSittable>true</isSittable>
    </building>
    <constructionSkillPrerequisite>6</constructionSkillPrerequisite>
    <researchPrerequisites>
      <li>VFEE_NobleDecorations</li>
    </researchPrerequisites>
  </ThingDef>
  
  <ThingDef ParentName="FurnitureBase">
    <defName>VFEE_ImperialBanner</defName>
    <label>imperial banner</label>
    <description>text.todo</description>
    <designationCategory>Misc</designationCategory>
    <altitudeLayer>BuildingOnTop</altitudeLayer>
    <passability>PassThroughOnly</passability>
    <fillPercent>0.35</fillPercent>
    <statBases>
      <MaxHitPoints>100</MaxHitPoints>
      <WorkToBuild>1000</WorkToBuild>
      <Mass>15</Mass>
      <Flammability>1.0</Flammability>
      <Beauty>5</Beauty>
    </statBases>
    <stuffCategories>
      <li>Fabric</li>
      <li>Leathery</li>
    </stuffCategories>
    <costStuffCount>75</costStuffCount>
    <costList>
      <WoodLog>25</WoodLog>
    </costList>
    <canOverlapZones>true</canOverlapZones>
    <pathCost>14</pathCost>
    <rotatable>false</rotatable>
    <size>(1,1)</size>
    <uiIconOffset>(0, 0.15)</uiIconOffset>
    <filthLeaving>Filth_Trash</filthLeaving>
    <building>
      <destroySound>BuildingDestroyed_Soft_Medium</destroySound>
      <destroyShakeAmount>0</destroyShakeAmount>
    </building>
    <researchPrerequisites>
      <li>VFEE_NobleDecorations</li>
    </researchPrerequisites>
    <graphicData>
      <drawSize>(2,2)</drawSize>
      <texPath>Furniture/ImperialBanner</texPath>
      <graphicClass>Graphic_Single</graphicClass>
      <shaderType>CutoutComplex</shaderType>
      <shadowData>
        <volume>(1,0.5,0.3)</volume>
        <offset>(0,0,-0.2</offset>
      </shadowData>
      <damageData>
        <rectN>(0.1, 0.1, 0.8, 0.5)</rectN>
        <rectS>(0.1, 0.1, 0.8, 0.5)</rectS>
        <rectE>(0.25, 0.5, 0.5, 0.5)</rectE>
        <rectW>(0.25, 0.5, 0.5, 0.5)</rectW>
      </damageData>
    </graphicData>
  </ThingDef>
  
