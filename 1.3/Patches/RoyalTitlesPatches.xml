<?xml version="1.0" encoding="utf-8" ?>
<Patch>
	<!--For adding later-->
	<Operation Class="PatchOperationAdd">
		<xpath>/Defs/RoyalTitleDef[defName="Count"]/grantedAbilities</xpath>
		<value>
			<li>VFEE_RoyalAddress</li>
			<li>VFEE_BestowFreeholder</li>
		</value>
	</Operation>
	
    <Operation Class="PatchOperationAttributeSet">
        <xpath>/Defs/RoyalTitleDef[defName="Duke"]</xpath>
        <attribute>ParentName</attribute>
        <value>BaseEmpireTitle</value>
    </Operation>
    <Operation Class="PatchOperationAttributeSet">
        <xpath>/Defs/RoyalTitleDef[defName="Duke"]</xpath>
        <attribute>Name</attribute>
        <value>DukeBase</value>
    </Operation>
    <Operation Class="PatchOperationAttributeSet">
        <xpath>/Defs/RoyalTitleDef[defName="Duke"]</xpath>
        <attribute>Abstract</attribute>
        <value>True</value>
    </Operation>
